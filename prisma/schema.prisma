generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Exercise {
  exerciseID String
  status String @default("incomplete")
  user String 
  @@unique([exerciseID,user])
}

model preference {
  uid      String    @unique
  darkmode  Boolean   @default(false)
}

model Users{
uid String @unique
token String
}